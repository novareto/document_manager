<!doctype html>
<html lang="en" class="h-100">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon"
    href="/fanstatic/adhoc/favicon.png"
    />
  <title>UV-REHA</title>
  <script src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc=" crossorigin="anonymous"></script>
</head>
<body data-with-sidebar="false">



  <header>
    <div tal:replace="structure slot: sitecap"> </div>
    <div tal:replace="structure slot: globalmenu"> </div>
    <div tal:replace="structure slot: navbar"> </div>
    <div tal:replace="structure slot: sidebar"> </div>
  </header>

  <section id="content" class="container">
    <div class="content container">

      <div tal:condition="messages" tal:repeat="message messages" tal:attributes="class string:mt-4 mb-4 alert alert-${message['type']}">
        <span tal:replace="message['body']" />
      </div>

      <div tal:replace="structure content"> </div>
    </div>
  </section>


  <footer id="portal-footer" class="footer mt-auto"> 
    <div tal:replace="structure slot: footer"> </div>
  </footer>
</body>
</html>
